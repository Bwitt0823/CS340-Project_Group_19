<html>
    {{#each foo}}
    {{this}}
    {{/each}}

    <h3>Current Front Office Members<h3>
    <table>
      <thead>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Role</th>
        <th>Team</th>
      </thead>
      <tbody>
        {{#each FrontOffice}}
        <tr>
          <td>{{First_Name}}</td>
          <td>{{Last_Name}}</td>
          <td>{{Role}}</td>
          <td>{{Team}}</td>
          <td><button onclick="deleteFrontOffice({{ID_Front_Office}})">Delete</button></td>
          <td><a href="/UpdateFrontOffice/{{ID_Office}}">Update</a></td>
        </tr>
        {{/each}}
      </tbody>
    </table>

    <h3>Add Front Office Member</h3>
    <form id="addFrontOffice" action="/FrontOffice" method="POST">
        First Name: <input type="text" name="First_Name"> <br>
        Last Name: <input type="text" name="Last_Name"> <br>
        Role: <input type="text" name="Role"> <br>
        Team: <select name="team">
            {{#each FrontOffice}}
            <option value="{{ID_Front_Office}}"> {{Team}} </option>
            {{/each}} </select> <br>
        <input type="submit" value="Submit">
    </form>
    </br>

</html>
